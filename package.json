{
  "name": "gim",
  "displayName": "gim",
  "version": "1.0.0",
  "description": "GIM (BLAZINGLY FAST PRIVACY FIRST LIGHTWEIGHT AGENTIC AI LEGACY CODE EXPLAINER MODEL). ENABLES CEOS TO FIRE ALL DEVELOPERS CIRCA 2025",
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.103.0"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "gim.acceptChanges",
        "title": "Accept Suggestion",
        "icon": {
          "light": "resources/light/check.svg",
          "dark": "resources/dark/check.svg"
        }
      },
      {
        "command": "gim.selection.docstring",
        "title": "GIM: Docstring from Selection"
      },
      {
        "command": "gim.selection.analyze",
        "title": "GIM: Analyze Selection"
      },
      {
        "command": "gim.selection.explain",
        "title": "GIM: Explain Selection"
      },
      {
        "command": "gim.file.analyze",
        "title": "GIM: Analyze Current File"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "gim.acceptChanges",
          "group": "navigation",
          "when": "isInDiffEditor && resourceScheme == gim-diff"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  }
}
